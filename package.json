{
  "name": "edgepicks",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run validate-env && npm run list-missing-env && next dev",
    "build": "npm run validate-env && next build",
    "start": "npm run validate-env && next start",
    "dev:local": "npm run validate-env && npm run list-missing-env && next dev --port 3000",

    "test": "node scripts/update-llms-log.test.js && node lib/logUiEvent.test.js && jest",
    "test:local": "tsx scripts/testLocal.ts",

    "validate-env": "tsx scripts/validateEnv.ts",
    "list-missing-env": "tsx scripts/listMissingEnv.ts",
    "test:metadata": "tsx scripts/audit-agent-metadata.ts",
    "sync-system-diagram": "tsx scripts/syncSystemDiagram.ts",
    "snap:test": "tsx scripts/uiSnapshot.ts",
    "postpush": "tsx scripts/log-llms-entry.ts",

    "postinstall": "node -e \"try{require.resolve('@types/react')}catch(e){console.warn('@types/react not found')}\"",
    "prepare": "husky",
    "lint": "next lint"
  },
  "dependencies": {
    "@octokit/rest": "^20.1.0",
    "@supabase/supabase-js": "^2.39.5",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.536.0",
    "next": "^14.2.31",
    "next-auth": "^4.24.11",
    "p-limit": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.3.4",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/next-auth": "^3.13.0",
    "@types/node": "^24.2.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.4.6",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  }
}
